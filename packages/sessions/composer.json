{
    "name": "vilagmuhely/sessions",
    "description": "Session management package for Laravel with Filament admin interface",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vilagmuhely\\Sessions\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "filament/filament": "^3.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vilagmuhely\\Sessions\\Providers\\SessionServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
