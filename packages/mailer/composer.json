{
    "name": "webtechsolutions/mailer",
    "description": "Email management package for Laravel with Filament admin interface",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Webtechsolutions\\Mailer\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/mail": "^12.0",
        "filament/filament": "^3.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webtechsolutions\\Mailer\\Providers\\MailerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
